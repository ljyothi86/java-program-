import java.util.*;
class ArrayListDemo
{
public static void main (String args[])
{
ArrayList<String>ar|=new ArrayList<String>();
ar|.add("apple");     ar|.add("guva");
ar|.("grapes");      ar|.add("banana");
ar|.add("mango");
System.out.println("ArrayList="+ar|);
ar|.remove("guva");   ar|remove(2);
System.out.println("arraylist elements=+ar|);
System.out.println("size=+arl.sixe());
Iterator it=ar|.iterator();
while(it.hasNext())
System.out.println(it.next());
}
}


